{
  "name": "animated-icons",
  "version": "0.1.0",
  "private": false,
  "description": "Beautiful animated SVG icons with motion/react",
  "author": "Abhij",
  "license": "MIT",
  "keywords": [
    "icons",
    "animated",
    "svg",
    "react",
    "motion",
    "framer-motion"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "build": "tsup",
    "registry:build": "shadcn build",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "next": "16.1.0",
    "next-themes": "^0.4.6",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "shadcn": "^3.6.2",
    "tabler": "^1.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4",
    "tsup": "^8.4.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
